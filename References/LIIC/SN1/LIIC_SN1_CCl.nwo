from nwgeom import *
geom = '''
    geometry noprint adjust
        zcoord
            bond  1 3   %f ci  constant
        end
    end
'''
x0=3.000
y0=1.801
xmax=2.171
ymax=3.000
npts=20
ymin = y0 - (ymax-y0)/npts
results = scan_input(geom, \
[ymin],
[ymax],
npts, 'dft', task_optimize)
fp=open("sn1_ccl.dat","w")
for i in range(0,len(results)):
    fp.write(f'{results[i][0][0]} {results[i][1]} \n')
fp.close()

